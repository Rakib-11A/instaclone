<div class="col-xl-8 col-md-6 col-sm-8 col-10" style="margin: auto; background-color: #f8f8f8; border-radius: 10px; padding: 15px; box-shadow: 0px 0px 2px 2px rgba(255, 255, 255, 0.1)">
<%= stylesheet_link_tag "post" %>

<p style="color: green"><%= notice %></p>

<% content_for :title, "Posts" %>


<div id="posts" class="text-center ">
  <% @posts.each do |post| %>
    <%= render post %>
  <% end %>
</div>

</div>


